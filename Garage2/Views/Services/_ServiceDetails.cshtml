@model IEnumerable<CategoryItems>

@{
    Layout = null;
}

@if (Model.Any())
{
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Category</th>
                <th scope="col">Item</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>

                    <td>@item.Category</td>
                    <td>@item.Items</td>
                </tr>
            }
        </tbody>
    </table>
 }else
{
   <p>No details</p>
}




